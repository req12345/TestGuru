<h1> Test: <%= @test.title %></h1>
<h2>Question: <%= @question.body %></h2>
<h2>Answers:</h2>
<%= link_to 'New answer', new_admin_question_answer_path(@test, @question) %>
<table>
  <tr>
    <th>Body</th>
    <th>Correct answer</th>
    <th colspan="2">Actions</th>
  </tr>
  <%= render partial: 'admin/answers/answer', collection: @question.answers %>

  </table>

<%= link_to 'Back', admin_test_path(@test) %>
