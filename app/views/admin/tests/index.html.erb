<h1>Fancy tests:</h1>
<%= link_to 'New test', new_admin_test_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Level</th>
    <th>Category</th>
    <th>Questions</th>
    <th>Author</th>
    <th colspan="3">Actions</th>
  </tr>
    <%= render @tests  %>
</table>



  <h2>Questions:</h2>

  <%= link_to "Create question", new_admin_question_path(@test) %>

<table>
  <thead>
  <tr>
    <th>Question body</th>
    <th>Question title</th>
    <th colspan="3">Actions</th>
  </tr>
  </thead>

  <%= render partial: 'admin/questions/question', collection: @test.questions %>
</table>
